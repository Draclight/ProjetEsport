@page
@model projetEsport.Areas.Admin.Pages.Licencies.EditModel

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<h4>Licencie</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Licencie.ID" />
            <div class="form-group">
                <label asp-for="Licencie.Pseudo" class="control-label"></label>
                <input asp-for="Licencie.Pseudo" class="form-control" />
                <span asp-validation-for="Licencie.Pseudo" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Licencie.Penom" class="control-label"></label>
                <input asp-for="Licencie.Penom" class="form-control" />
                <span asp-validation-for="Licencie.Penom" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Licencie.Nom" class="control-label"></label>
                <input asp-for="Licencie.Nom" class="form-control" />
                <span asp-validation-for="Licencie.Nom" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Licencie.IdUtilisateur" class="control-label"></label>
                <input asp-for="Licencie.IdUtilisateur" class="form-control" />
                <span asp-validation-for="Licencie.IdUtilisateur" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Licencie.CreeLe" class="control-label"></label>
                <input asp-for="Licencie.CreeLe" class="form-control" />
                <span asp-validation-for="Licencie.CreeLe" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Licencie.ModifieeLe" class="control-label"></label>
                <input asp-for="Licencie.ModifieeLe" class="form-control" />
                <span asp-validation-for="Licencie.ModifieeLe" class="text-danger"></span>
            </div>
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="Licencie.PremierConnexion" /> @Html.DisplayNameFor(model => model.Licencie.PremierConnexion)
                </label>
            </div>
            <div class="form-group">
                <label asp-for="Licencie.EquipeID" class="control-label"></label>
                <select asp-for="Licencie.EquipeID" class="form-control" asp-items="ViewBag.EquipeID"></select>
                <span asp-validation-for="Licencie.EquipeID" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-page="./Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
