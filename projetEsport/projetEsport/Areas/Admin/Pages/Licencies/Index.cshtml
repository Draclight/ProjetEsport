@page
@model projetEsport.Areas.Admin.Pages.Licencies.IndexModel

@{
    ViewData["Title"] = "Licenciés";
}

<h1>@ViewData["Title"]</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Licencie[0].Pseudo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Licencie[0].Prenom)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Licencie[0].Nom)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Licencie[0].IdUtilisateur)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Licencie[0].CreeLe)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Licencie[0].ModifieeLe)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Licencie[0].Equipe)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Licencie) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Pseudo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Prenom)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nom)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdUtilisateur)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreeLe)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ModifieeLe)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Equipe.Nom)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.ID">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.ID">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
